{
	"name": "Video Downloader",
	"description": "Chrome plugin that adds video download functionality for Facebook",
	"version": "0.1.1",
	"manifest_version": 2,
	"icons": {
		"128": "download.png"
	},
	"permissions": [
		"<all_urls>",
		"downloads"
	],
	"background": {
		"scripts": [
			"index.js"
		]
	},
	"content_scripts": [
		{
			"matches": [
				"<all_urls>"
			],
			"js": [
				"content-script.js"
			],
			"all_frames": true
		}
	]
}
